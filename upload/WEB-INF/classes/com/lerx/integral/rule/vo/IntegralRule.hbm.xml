<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lerx.integral.rule.vo">
	<class name="IntegralRule" table="integral_rule" dynamic-update="true">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="name" length="50" />
		<property name="localPostion"  />
		<property name="createTime"  />
		<property name="valueOfAdd" />
		<property name="valueOfDel" />
		<property name="valueOfReg" />
		<property name="valueOfLogin" />
		<property name="state" />
	</class>
</hibernate-mapping>
