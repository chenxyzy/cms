		<property name="dateFormatOnLine"></property>
		<property name="dateFormatInText"></property>
		<property name="majorLoopCodeInLump" type="text"></property>
		<property name="minorLoopCodeInLump" type="text"></property>
		<property name="editAreaCode" type="text"></property>
		<property name="searchAreaCode" type="text"></property>
		<property name="specialCode1" type="text"></property>
		<property name="specialCode2" type="text"></property>
		<property name="specialCode3" type="text"></property>
